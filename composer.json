{
    "name": "orchestra/http",
    "description": "HTTP Component for Orchestra Platform",
    "keywords": ["orchestra-platform", "orchestral", "http"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Http\\": ""
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "orchestra/contracts": "^7.0",
        "orchestra/extension": "^7.0",
        "orchestra/support": "^7.0"
    },
    "suggest": {
        "orchestra/extension": "Allow to use Orchestra Extension route resolver (^7.0).",
        "hashids/hashids": "Allow to use HashId as a URL id replacement (^1.0)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    },
    "support": {
        "issues": "https://github.com/orchestral/kernel/issues"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
